# 声明严格模式
'use strict'

# 定义项目名称和版本
name: jinza-management-system
version: 1.0.0

# 环境变量
NODE_ENV=production

# 服务器配置
HOST=localhost
PORT=3000

# API基础URL - Render后端服务的URL (不带/api结尾，我们在请求时添加)
VITE_API_BASE_URL=https://jinza-backend.onrender.com/api

# 是否启用mock数据
VITE_USE_MOCK=false

# 资源公共路径
VITE_PUBLIC_PATH=/

# 上传文件大小限制
VITE_MAX_FILE_SIZE=10485760